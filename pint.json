{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "modernize_strpos": true,
        "no_unused_imports": true,
        "ordered_imports": {
            "sort_algorithm": "alpha",
            "imports_order": [
                "class",
                "function",
                "const"
            ]
        },
        "trailing_comma_in_multiline": {
            "elements": [
                "arrays",
                "arguments",
                "parameters"
            ]
        }
    }
}
